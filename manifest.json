{

  "manifest_version": 2,
  "name": "Remember settings for weblab",
  "author": "Thijs Raymakers",
  "version": "1.1",

  "description": "Remember the settings of the editors in weblab",

  "content_scripts": [
    {
      "matches": ["*://weblab.tudelft.nl/*"],
      "js": ["vimify.js"]
    }
  ],
  
  "permissions": ["storage"],

  "applications": {
    "gecko": {
      "id": "remember_weblab_settings@thijs.sh"
    }
  }


}
